                              <td class="left">{{ text_options_name }}</td> {% endif %}
                            {% if k == 'L_TAXAMT' %}
                              <td class="left">{{ text_tax_amt }}</td> {% endif %}
                            {% if k == 'L_CURRENCYCODE' %}
                              <td class="left">{{ text_currency_code }}</td> {% endif %}
